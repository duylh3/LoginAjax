
@{
    ViewBag.Title = "History";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

@section Scripts{
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/plugins/mustache.min.js"></script>
    <script src="~/Scripts/jquery.twbsPagination.min.js"></script>
    <script src="~/Scripts/app/controller/historyjs.js"></script>
}

<div class="row">
    <div class="row col-md-12 alert alert-info" style="margin-left:2px; width: 98%">
        <div class="col-md-3">
            Thống kê danh sách câu hỏi
        </div>
        <div class="col-md-1">
            ||
        </div>
        <div class="col-md-2" style="margin-left:-30px">
            <form id="myForm" action="/QA/ExportExcel" method="post">
                <input type="submit" value="Xuất dữ liệu" class="btn btn-link" style="padding-top:0px"/>
            </form>
        </div>
    </div>
    <table class="table table-striped table-bordered" style="width:98%; margin-left: 2px">
        <thead>
            <tr>
                <th>#</th>
                <th>Câu hỏi</th>
                <th>Login ID</th>
                <th>Người hỏi</th>
                <th>Enter Date</th>
                <th>Work Group</th>
                <th>Ngày hỏi</th>
            </tr>
        </thead>
        <tbody id="tbl-content"></tbody>
    </table>
    <div id="pagination" class="pagination">
    </div>
</div>

<script id="table-template" type="x-tmpl-mustache">
    <tr>
        <td class="col-seq">{{Sequence}}</td>
        <td>{{{Question}}}</td>
        <td>{{LoginId}}</td>
        <td>{{Name}}</td>
        <td>{{ENTER_DATE}}</td>
        <td>{{WORKGROUP}}</td>
        <td>{{CreatedDate}}</td>
    </tr>
</script>

